{{!< default}}
<main>
	<div class="post-list">
		{{# each data.posts}}
			<article class="post-card">
				<a href="{{href}}" title="Article title">
					<div class="thumbnail">
					<img src="{{imageURL}}" alt="Article title goes here">
					</div>
					<h2>{{{title}}}</h2>
				</a>
				<p>{{{content.brief}}}</p>
			</article>
		{{/each}}
	</div>
	<!-- <div class="pagination">
	pagination goes here
	</div> -->
</main>
